# Application
NODE_ENV=development
PORT=3000

# Data Processing
FROM=2025-08-01
TO=2025-08-31
BATCH_SIZE=100

# File System
WORKING_DIR=/path/to/source/files
OUTPUT_DIR=/path/to/output/files
ARCHIVE_DIR=/path/to/archive

# Database
DB_PATH=./sqlite/data.sqlite
MAPPING_NAME=default

# HTTP Endpoint
API_ENDPOINT=http://your-api-endpoint.com
API_KEY=your-api-key
API_TIMEOUT=30000

# Logging
LOG_LEVEL=info
LOG_FORMAT=json  # json or simple
